{% extends 'base.html.twig' %}

{% block body %}
    {% if not app.user or app.user.admin %}
        {{ include('inc/register.html.twig') }}
    {% else %}
        {{ include('inc/edit.html.twig') }}
    {% endif %}
{% endblock %}

{% block title %}
    {% if app.user %}
        {{ parent() }} | Edit
    {% else %}
        {{ parent() }} | Register
    {% endif %}
{% endblock %}

